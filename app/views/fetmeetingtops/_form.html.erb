<%= semantic_form_for @fetmeetingtop do |f| %>
  <%= f.inputs do %>
    <%= f.input :title %>
    <%= f.input :text %>
    <%= f.input :discussion , as: :tinymce_text %>
    <%= f.input :ersteller %>
    <%= f.input :fetmeeting_id %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
<%= tinymce %>
