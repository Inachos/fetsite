<%= semantic_form_for @fetmeeting do |f| %>
  <%= f.inputs do %>
    <%= f.input :tnlist %>
    <%= f.input :typ %>
	 <%= f.semantic_fields_for :calentry , @fetmeeting.calentry do |calentry| %>
      <%= render 'calentries/nested_fields', :f => calentry %> 
      <% end %>
   
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
